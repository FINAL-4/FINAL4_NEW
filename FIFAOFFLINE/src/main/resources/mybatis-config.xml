<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<!-- 클래스들을 미리 만들어 놔야 별칭 지정 한 것에서 에러가 발생하지 않는다. -->
	<typeAliases>
	
		<!-- ======================= 강현모 ========================= -->
		<typeAlias type="com.kh.FIFAOFFLINE.match.model.vo.Match" alias="Match"/>
		<typeAlias type="com.kh.FIFAOFFLINE.match.model.vo.AppMatch" alias="AppMatch"/>
		<typeAlias type="com.kh.FIFAOFFLINE.match.model.vo.SmsInfo" alias="SmsInfo"/>
		<typeAlias type="com.kh.FIFAOFFLINE.match.model.vo.ScoreInfo" alias="ScoreInfo"/>
		<typeAlias type="com.kh.FIFAOFFLINE.tournament.model.vo.Tournament" alias="Tournament"/>
		<typeAlias type="com.kh.FIFAOFFLINE.tournament.model.vo.TournamentInfo" alias="TournamentInfo"/>
		<typeAlias type="com.kh.FIFAOFFLINE.tournament.model.vo.TournamentSche" alias="TournamentSche"/>
		
		<!-- ======================= 배성용 ========================= -->
		<typeAlias type="com.kh.FIFAOFFLINE.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.FIFAOFFLINE.notice.model.vo.Notice" alias="Notice"/>
		
		
		
		
		
		<!-- ======================= 서재웅 ========================= -->
		<typeAlias type="com.kh.FIFAOFFLINE.player.model.vo.P_RECRUIT" alias="P_RECRUIT"/>
		<typeAlias type="com.kh.FIFAOFFLINE.player.model.vo.P_LIST" alias="P_LIST"/>
		<typeAlias type="com.kh.FIFAOFFLINE.player.model.vo.P_ENROLL" alias="P_ENROLL"/>
		
		
		
		
		
		<!-- ======================= 안효찬 ========================= -->
		<typeAlias type="com.kh.FIFAOFFLINE.team.model.vo.Team" alias="Team"/>
		<typeAlias type="com.kh.FIFAOFFLINE.team.model.vo.TeamAD" alias="TeamAD"/>
		<typeAlias type="com.kh.FIFAOFFLINE.team.model.vo.TeamJoinedMember" alias="TeamJoinedMember"/>
		<typeAlias type="com.kh.FIFAOFFLINE.team.model.vo.TeamMember" alias="TeamMember"/>
		<typeAlias type="com.kh.FIFAOFFLINE.team.model.vo.MyTeam" alias="MyTeam"/>
		<typeAlias type="com.kh.FIFAOFFLINE.team.model.vo.TeamFilter" alias="TeamFilter"/>
		
		
		
		
		<!-- ======================= 이성호 ========================= -->
		<typeAlias type="com.kh.FIFAOFFLINE.member.model.vo.Member" alias="Member"/>
		
		
		
		
		
		
	</typeAliases>
	
	<!-- sql문들을 지는 mapper.xml들을 등록 -->
	<mappers>
		<!-- ======================= 배성용 ========================= -->
		<mapper resource="mappers/board-mapper.xml"/>
		<mapper resource="mappers/notice-mapper.xml"/>
		
		
		
		
		
		<!-- ======================= 강현모 ========================= -->
		<mapper resource="mappers/match-mapper.xml"/>
		<mapper resource="mappers/tournament-mapper.xml"/>
		
		
		
		
		
		<!-- ======================= 서재웅 ========================= -->
		<mapper resource="mappers/player-mapper.xml"/>
		
		
		
		
		
		
		<!-- ======================= 안효찬 ========================= -->
		<mapper resource="mappers/team-mapper.xml"/>
		
		
		
		
		
		
		<!-- ======================= 이성호 ========================= -->
		<mapper resource="mappers/member-mapper.xml"/>
		
		
		
		
		
		
		
	</mappers>
</configuration>
